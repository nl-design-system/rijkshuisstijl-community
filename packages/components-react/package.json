    "test": "mkdir -p pages && NODE_OPTIONS=--experimental-vm-modules jest --verbose",
    "watch:test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --watch"
  },
  "main": "./dist/index.cjs.js",
