    "@prettier/sync": "0.6.1",
    "@rijkshuisstijl-community/assets": "workspace:*",
    "@rijkshuisstijl-community/components-css": "workspace:*",
    "@rijkshuisstijl-community/components-react": "workspace:*",
    "@rijkshuisstijl-community/components-twig": "workspace:*",
    "@rijkshuisstijl-community/design-tokens": "workspace:*",
    "@rijkshuisstijl-community/font": "workspace:*",
    "@rijkshuisstijl-community/internal-tooling": "workspace:*",
    "@rijkshuisstijl-community/web-components": "workspace:*",
    "@storybook/addon-a11y": "9.1.10",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-links": "9.1.10",
    "@storybook/addon-themes": "9.1.6",
    "@storybook/cli": "9.1.10",
    "@storybook/react-vite": "9.1.10",
    "@storybook/web-components-vite": "9.1.10",
    "@tabler/icons-react": "3.35.0",
    "@tabler/icons-sprite": "3.35.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@types/twig": "1.12.17",
    "@utrecht/component-library-react": "10.3.2",
    "@utrecht/components": "7.4.0",
    "@utrecht/page-body-react": "1.0.6",
    "@utrecht/page-layout-react": "1.0.6",
    "@utrecht/root-react": "1.0.6",
    "@utrecht/web-component-library-react": "3.0.9",
    "@whitespace/storybook-addon-html": "8.0.2",
    "axe-playwright": "2.2.2",
    "chromatic": "11.29.0",
    "clsx": "2.1.1",
    "drupal-attribute": "1.2.1",
    "eslint-plugin-storybook": "9.1.10",
    "jest": "29.7.0",
    "prettier": "3.6.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-syntax-highlighter": "15.6.6",
    "remark": "15.0.1",
    "remark-normalize-headings": "4.0.0",
    "require-from-string": "2.0.2",
    "rimraf": "6.0.1",
    "storybook": "9.1.10",
    "storybook-addon-pseudo-states": "9.1.10",
    "tslib": "2.8.1",
