{
  "rhc": {
    "sidenav": {
      "display": {
        "value": "flex",
        "type": "other"
      },
      "link": {
        "line-height": {
          "value": "{rhc.line-height.body}",
          "type": "lineHeights"
        },
        "icon": {
          "size": {
            "value": "{rhc.size.icon.functional}",
            "type": "sizing"
          },
          "margin-inline": {
            "value": "{rhc.space.100}",
            "type": "spacing"
          }
        },
        "font-size": {
          "value": "{rhc.font-size.body.default}",
          "type": "fontSizes"
        },
        "font-family": {
          "value": "{rhc.font-family.primary}",
          "type": "fontFamilies"
        },
        "font-weight": {
          "value": "{rhc.font-weight.default}",
          "type": "fontWeights"
        },
        "current": {
          "font-weight": {
            "value": "{rhc.font-weight.strong}",
            "type": "fontWeights"
          },
          "color": {
            "value": "{rhc.color.foreground.default}",
            "type": "color"
          }
        },
        "color": {
          "value": "{rhc.color.foreground.link}",
          "type": "color"
        },
        "active": {
          "color": {
            "value": "{rhc.color.paars.500}",
            "type": "color"
          },
          "text-decoration": {
            "value": "underline",
            "type": "textDecoration"
          }
        },
        "focus": {
          "border-radius": {
            "value": "{rhc.border-radius.md}",
            "type": "borderRadius"
          },
          "border-width": {
            "value": "{rhc.border-width.m}",
            "type": "borderWidth"
          },
          "border-style": {
            "value": "solid",
            "type": "other"
          },
          "border-color": {
            "value": "{rhc.color.primary.500}",
            "type": "color"
          },
          "background-color": {
            "value": "none",
            "type": "color"
          },
          "color": {
            "value": "{rhc.color.foreground.link}",
            "type": "color"
          },
          "text-decoration": {
            "value": "None",
            "type": "textDecoration"
          }
        },
        "hover": {
          "color": {
            "value": "{rhc.color.foreground.link}",
            "type": "color",
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "darken",
                  "value": "0.3",
                  "space": "lch"
                }
              }
            }
          },
          "text-decoration": {
            "value": "underline",
            "type": "textDecoration"
          }
        },
        "padding-block-start": {
          "value": "{rhc.space.150}",
          "type": "spacing"
        },
        "padding-block-end": {
          "value": "{rhc.space.150}",
          "type": "spacing"
        },
        "text-decoration": {
          "value": "None",
          "type": "textDecoration"
        },
        "column-gap": {
          "value": "{rhc.space.100}",
          "type": "spacing"
        }
      }
    }
  }
}